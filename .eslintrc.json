{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "plugins": [
    "html"
  ],
  "settings": {
    "html/indent": "+2"
  },
  "extends": "airbnb",
  "rules": {
    "no-var": 0,
    "one-var": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": [2, "never"],
    "strict": [2, "global"],
    "comma-dangle": 0,
    "wrap-iife": [2, "inside"],
    "space-in-parens": 0,
    "computed-property-spacing": 0
  }
}
